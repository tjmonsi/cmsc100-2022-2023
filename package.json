{
  "name": "cmsc100-2022-2023",
  "version": "0.0.1",
  "description": "Todo App",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "npm-run-all --parallel nodemon webpack",
    "nodemon": "nodemon src/index --ignore db.json",
    "semistandard": "semistandard --fix",
    "tap": "c8 --lines 80 --functions 80 --branches 80 --statements 80 --check-coverage --per-file tap test/**/*.test.js test/**/**/*.test.js --no-coverage",
    "test": "npm run semistandard && npm run tap",
    "webpack": "npx webpack -c webpack.dev.cjs --watch",
    "webpack:build": "npx webpack -c webpack.dev.cjs",
    "webpack:build:production": "npx webpack -c webpack.prod.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjmonsi/cmsc100-2022-2023.git"
  },
  "keywords": [
    "Node",
    "JS",
    "Web",
    "Server"
  ],
  "author": "TJ Monserrat",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tjmonsi/cmsc100-2022-2023/issues"
  },
  "homepage": "https://github.com/tjmonsi/cmsc100-2022-2023#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "babel-loader": "^9.1.0",
    "c8": "^7.12.0",
    "chance": "^1.1.8",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "8.19.0",
    "eslint-config-semistandard": "16.0.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-chai-expect": "3.0.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-mocha": "^10.0.5",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-standard": "4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "must": "^0.13.4",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "semistandard": "^16.0.1",
    "tap": "^16.3.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.8.0",
    "worker-loader": "^3.0.8"
  },
  "semistandard": {
    "plugins": [
      "html",
      "standard",
      "promise",
      "mocha",
      "chai-expect"
    ],
    "parser": "@babel/eslint-parser",
    "env": [
      "mocha"
    ],
    "globals": [
      "describe",
      "it",
      "after",
      "before",
      "expect"
    ]
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/jwt": "^6.3.2",
    "@fastify/secure-session": "^5.2.0",
    "@fastify/sensible": "^5.1.1",
    "@fastify/static": "^6.5.1",
    "@fastify/swagger": "^7.6.1",
    "@tjmonsi/small-router": "^0.0.3",
    "bcrypt": "^5.1.0",
    "comlink": "^4.3.1",
    "fastify": "^4.7.0",
    "fastify-openapi-glue": "^4.0.1",
    "lit": "^2.4.1",
    "uuid": "^9.0.0"
  }
}
